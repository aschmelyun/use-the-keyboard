        {
            "name": "Navigation",
            "shortcuts": [
                {
                    "description": "Move focus to the next element",
                    "keys": ["Tab"]
                },
                {
                    "description": "Move focus to the previous element",
                    "keys": ["Shift", "Tab"]
                },
                {
                    "description": "Move focus through messages in a channel or DM",
                    "keys": ["Up/Down"]
                },
                {
                    "description": "Move focus to the next section",
                    "keys": ["F6"]
                },
                {
                    "description": "Move focus to the previous section",
                    "keys": ["Shift", "F6"]
                },
                {
                    "description": "Take action or 'click' on buttons",
                    "keys": ["Space"]
                },
                {
                    "description": "Take action or 'click' on links",
                    "keys": ["Enter"]
                },
                {
                    "description": "Close windows, menus, or the message field",
                    "keys": ["Esc"]
                },
                {
                    "description": "Zoom in/out",
                    "keys": ["Ctrl", "+/-"]
                },
                {
                    "description": "Scroll up",
                    "keys": ["Home/PgUp"]
                },
                {
                    "description": "Scroll down",
                    "keys": ["End/PgDown"]
                },
                {
                    "description": "Quick Switcher",
                    "keys": ["Ctrl", "K"]
                },
                {
                    "description": "Open direct messages menu",
                    "keys": ["Ctrl", "Shift", "K"]
                },
                {
                    "description": "Browse all channels",
                    "keys": ["Ctrl", "Shift", "L"]
                },
                {
                    "description": "Open All Threads view",
                    "keys": ["Ctrl", "Shift", "T"]
                },
                {
